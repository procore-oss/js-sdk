{
  "name": "@procore/js-sdk",
  "version": "4.0.2",
  "description": "A wrapper for the procore API",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "types": "dist/types",
  "homepage": "https://github.com/procore/js-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/procore/js-sdk.git"
  },
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "test": "yarn clean && yarn compile && yarn test:once",
    "test:once": "nyc mocha -r ts-node/register test/**/*.spec.ts",
    "test:file": "nyc mocha -r ts-node/register",
    "test:coverage": "yarn clean && yarn compile && nyc --reporter=lcov mocha -r ts-node/register test/**/*.spec.ts",
    "clean": "rimraf dist types",
    "watch": "tsc --watch",
    "compile": "yarn writeVer && tsc",
    "writeVer": "cp package.json lib/version.json"
  },
  "author": "Procore Tech <insights@procore.com> (http://procore.com)",
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^3.0.0",
    "qs": "^6.11.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/core-js": "^2.5.5",
    "@types/fetch-mock": "^7.3.5",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.5",
    "@types/qs": "^6.9.7",
    "@types/sinon": "^10.0.13",
    "chai": "^4.3.7",
    "fetch-mock": "^9.11.0",
    "jsdom": "^21.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^15.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
