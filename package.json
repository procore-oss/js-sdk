    "string": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/core-js": "^0.9.35",
    "@types/fetch-mock": "^5.8.0",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/mocha": "^2.2.38",
    "@types/qs": "^6.2.30",
    "@types/ramda": "^0.0.3",
    "@types/sinon": "^2.3.2",
    "@types/string": "^0.0.28",
    "chai": "^3.5.0",
